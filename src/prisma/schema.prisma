// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Artist {
  id      Int     @id @default(autoincrement())
  name    String
  link    String?
  picture String?
}

model Music {
  id            Int     @id @default(autoincrement())
  title         String
  picture       String?
  artistName    String?
  artistPicture String?
  link          String?
  preview       String?
  album         String?
}
